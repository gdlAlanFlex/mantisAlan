{
  "type": "object",
  "comment": "",
  "properties": {
    "createissue": {
      "type": "action",
      "selector": ".button[value=\"Submit Report\"]",
      "control": "action.button"
    },
    "category": {
      "type": "select",
      "selector": "[name=\"category_id\"]",
      "control": "select"
    },
    "summary": {
      "type": "string",
      "selector": "[name=\"summary\"]",
      "control": "text"
    },
    "description": {
      "type": "string",
      "control": "text",
      "selector": "[name=\"description\"]"
    },
    "isReportIssue": {
      "type": "identifier",
      "selector": "[name=\"report_bug_form\"]",
      "control": "exists"
    }
  },
  "connect": [
    {
      "match": [
        "isReportIssue"
      ],
      "screens": [
        "reportissue"
      ]
    }
  ]
}
