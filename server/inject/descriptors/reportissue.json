          },
          "summary": {
            "type": "string",
            "selector": "[name=\"summary\"]",
            "control": "text"
          },
          "photo": {
            "type": "string"
          },
          "private": {
            "type": "boolean",
            "selector": "[name=\"view_state\"]",
            "control": "radio"
          },
          "public": {
            "type": "string",
            "selector": "[name=\"view_state\"]",
            "control": "radio"
          }
        }
      },
      "selectorAll": "[class=\"form-title\"]"
    },
    "newissueid": {
      "type": "identifier",
      "selector": "[class=\"form-title\"]",
      "control": "exists",
      "selectContains": "Enter Report Details"
    }
